{
    "name": "fatiru/unoconv",
    "description": "Convert documents using unoconv inside a Laravel application.",
    "keywords": [
        "converter",
        "documents",
        "unoconv",
        "laravel"
    ],
    "homepage": "https://github.com/fatiru/unoconv",
    "license": "MIT",
    "authors": [
        {
            "name": "Fatiru",
            "email": "dev@fanetz.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|6.*",
        "php" : ">=7.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Angusm73\\Unoconv\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Angusm73\\Unoconv\\UnoconvServiceProvider"
            ],
            "aliases": {
                "Unoconv": "Angusm73\\Unoconv\\Facades\\Unoconv"
            }
        }
    }
}
